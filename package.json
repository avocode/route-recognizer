    "do-you-even-bench": "^1.0.5",
    "ember-cli": "^3.15.2",
    "glob": "^7.1.6",
    "jshint": "^2.11.0",
    "qunitjs": "^2.4.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "tree-sync": "^2.0.0",
    "tslint": "^6.0.0"
